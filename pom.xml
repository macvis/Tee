<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>tee.javaLearning</groupId>
    <artifactId>tee.0.0.0.1</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>

    <developers>
        <developer>
            <name>TerrenceWen</name>
            <url>https://github.com/macvis/</url>
            <email>macvis@126.com</email>
        </developer>
    </developers>
    <modules>
        <!-- head first 设计模式，原书中的部分教学代码 -->
        <module>head-first-design-pattern</module>
        <!-- java编程思想，部分实战代码 -->
        <module>thinking-in-java</module>
        <module>redis-learning</module>
        <!-- java8实战，书中的部分代码 -->
        <module>java8-lambda</module>
        <!-- 瞎玩的代码 -->
        <module>main</module>
        <!-- java并发编程实战 -->
        <module>java-concurrency-in-practice</module>
        <!-- java8实战 -->
        <module>java-8-in-action</module>
        <module>algurithum</module>
    </modules>

    <properties>
        <maven.compiler.source>1.8</maven.compiler.source>
        <maven.compiler.target>1.8</maven.compiler.target>
    </properties>

    <!-- 阿里云镜像 -->
    <repositories>
        <repository>
            <id>aliyun</id>
            <name>aliyun</name>
            <url>http://maven.aliyun.com/nexus/content/groups/public</url>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>never</updatePolicy>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
    </repositories>

    <pluginRepositories>
        <pluginRepository>
            <id>aliyun</id>
            <name>aliyun</name>
            <url>http://maven.aliyun.com/nexus/content/groups/public</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </pluginRepository>
    </pluginRepositories>


    <dependencies>
